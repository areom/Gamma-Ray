class Test:
  public:
    init():
      super()

    void interact():
      Printer p := system.out
      Integer i := 5
      Float f := 1.5
      p.printString("Sum of integer + float = ")
      p.printFloat(i.toF() + f)
      p.printString("\n")

  main(System system, String[] args):
    Test test := new Test()
    test.interact()
