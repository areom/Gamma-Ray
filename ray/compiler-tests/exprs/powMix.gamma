class IOTest:
  public:
    init():
      super()

    void interact():
      Printer p := system.out
      Integer i := 5
      Float f := 1.5
      p.printString("integer ^ float = ")
      p.printFloat(i.toF() ^ f)
      p.printString("\n")

  main(System system, String[] args):
    IOTest test := new IOTest()
    test.interact()
