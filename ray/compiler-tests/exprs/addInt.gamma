class Test:
  public:
    Integer a
    Integer b
    Integer c

    init():
      super()
      a := 1
      b := 2
      c := 3

    Integer overview():
      Integer success := a+b
      return success 

  main(System system, String[] args):
    Test ab := new Test()
    Printer p := system.out
    p.printString("Sum of integer = ")
    p.printInteger(ab.overview())
    p.printString("\n")
