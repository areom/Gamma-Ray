class IOTest:
  public:
    Integer a
    Integer b
    Integer c

    init():
      super()
      a := 1
      b := 2
      c := 0

    Integer overview():
      Integer success := 0
      system.out.printInteger(refine toExtra(a,b) to Integer)
/*      system.out.printInteger(success)*/
      return success 

class DIOTest extends IOTest:
  refinement:
    Integer overview.toExtra(Integer a, Integer b):
      Integer success :=  a + b
      Printer p := new Printer(true)
      p.printInteger(a)
      p.printInteger(b)
      p.printInteger(c)
      return success 
  public:
    Integer a1
    Integer b1
    Integer c1

    init():
      super()
      a1 := 1
      b1 := 2
      c1 := 0
class Main:
  public:
    init():
      super()

  main(System system, String[] args):
    IOTest ab := new IOTest()
    Printer p := system.out
    p.printString("Sum of integer = ")
    ab.overview()
/*    p.printInteger(ab.overview())*/
    p.printString("\n")
