class FuncTest:
  public:
      Integer a

      init():
          super()
          a := 1

  private:
      Integer incre_a(Integer b):
          a := a + b
          return a

      Integer incre_a_twice(Integer b):
          incre_a(b)
          incre_a(b)
          return a

  main(System system, String[] args):
      FuncTest test := new FuncTest()

